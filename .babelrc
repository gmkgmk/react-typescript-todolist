{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    ["@babel/preset-react"]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "import",
      { "libraryName": "antd", "libraryDirectory": "es", "style": true },
      "ant"
    ]
  ]
}
